CREATE TABLE ClickBookkeeper (
    uuid TEXT PRIMARY KEY NOT NULL,
    timestamp INTEGER NOT NULL
);

-- CRUD for ClickBookkeeper

select:
SELECT *
FROM ClickBookkeeper
WHERE uuid = ?
LIMIT 1;

upsert:
INSERT OR REPLACE INTO ClickBookkeeper
VALUES ?;

delete:
DELETE FROM ClickBookkeeper
WHERE uuid = ?;

deleteAll:
DELETE FROM ClickBookkeeper;
